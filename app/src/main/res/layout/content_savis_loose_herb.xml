<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/savisLooseHerbContentLayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="65dp">
    <Button
        android:id="@+id/buttonSavisLooseHerbChocolate"
        android:drawableStart="@drawable/ic_unknown"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:text="@string/Savis_loose_herb_chocolate"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"/>
    <Button
        android:id="@+id/buttonSavisLooseHerbExotic"
        android:drawableStart="@drawable/ic_unknown"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:text="@string/Savis_loose_herb_exotic"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"/>
    <Button
        android:id="@+id/buttonSavisLooseHerbJava"
        android:drawableStart="@drawable/ic_unknown"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:text="@string/Savis_loose_herb_java"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"/>
    <Button
        android:id="@+id/buttonSavisLooseHerbPeppermint"
        android:drawableStart="@drawable/ic_unknown"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:text="@string/Savis_loose_herb_peppermint"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"/>
</LinearLayout>
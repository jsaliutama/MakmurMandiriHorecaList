<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/gryphonOolongContentLayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="65dp">
    <Button
        android:id="@+id/buttonGryphonLilyOolong"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:drawableStart="@drawable/gryphon_lily_s"
        android:text="@string/Gryphon_oolong_lily"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"/>
    <Button
        android:id="@+id/buttonGryphonTempleOolong"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:drawableStart="@drawable/gryphon_temple_s"
        android:text="@string/Gryphon_oolong_temple"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"/>
</LinearLayout>
<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:id="@+id/savisBagFruitContentLayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="65dp"
        android:orientation="vertical">
        <Button
            android:id="@+id/buttonSavisBagFruitBlackcurrant"
            android:drawableStart="@drawable/ic_unknown"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:text="@string/Savis_bag_fruit_blackcurrant"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"/>
        <Button
            android:id="@+id/buttonSavisBagFruitLemon"
            android:drawableStart="@drawable/ic_unknown"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:text="@string/Savis_bag_fruit_lemon"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"/>
        <Button
            android:id="@+id/buttonSavisBagFruitLychee"
            android:drawableStart="@drawable/ic_unknown"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:text="@string/Savis_bag_fruit_lychee"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"/>
        <Button
            android:id="@+id/buttonSavisBagFruitMango"
            android:drawableStart="@drawable/ic_unknown"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:text="@string/Savis_bag_fruit_mango"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"/>
        <Button
            android:id="@+id/buttonSavisBagFruitPomegranate"
            android:drawableStart="@drawable/ic_unknown"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:text="@string/Savis_bag_fruit_pomegranate"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"/>
    </LinearLayout>
</ScrollView>
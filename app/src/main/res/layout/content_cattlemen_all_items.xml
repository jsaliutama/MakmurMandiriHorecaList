<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/cattlemenContentLayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="65dp"
    android:paddingBottom="10dp"
    android:orientation="vertical">
    <Button
        android:id="@+id/buttonCattlemenCarolina"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/Cattlemen_carolina"
        android:drawableStart="@drawable/cattlemen_carolina_s"/>
    <Button
        android:id="@+id/buttonCattlemenKansas"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/Cattlemen_kansas"
        android:drawableStart="@drawable/cattlemen_kansas_s"/>
    <Button
        android:id="@+id/buttonCattlemenMemphis"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/Cattlemen_memphis"
        android:drawableStart="@drawable/cattlemen_memphis_s"/>
    <Button
        android:id="@+id/buttonCattlemenMississippi"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/Cattlemen_mississippi"
        android:drawableStart="@drawable/cattlemen_mississippi_s"/>
    <Button
        android:id="@+id/buttonCattlemenSmoke"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/Cattlemen_smoke"
        android:drawableStart="@drawable/cattlemen_smoky_s"/>
</LinearLayout>
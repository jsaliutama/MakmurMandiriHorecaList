<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/gryphonRooibosContentLayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="65dp">
    <Button
        android:id="@+id/buttonGryphonAztecaRooibos"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:drawableStart="@drawable/gryphon_azteca_s"
        android:text="@string/Gryphon_rooibos_azteca"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"/>
    <Button
        android:id="@+id/buttonGryphonCobaRooibos"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:drawableStart="@drawable/gryphon_coba_s"
        android:text="@string/Gryphon_rooibos_coba"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"/>
    <Button
        android:id="@+id/buttonGryphonMogamboRooibos"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:drawableStart="@drawable/gryphon_mogambo_s"
        android:text="@string/Gryphon_rooibos_mogambo"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"/>
    <Button
        android:id="@+id/buttonGryphonTomatinoRooibos"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:drawableStart="@drawable/gryphon_tomatino_s"
        android:text="@string/Gryphon_rooibos_tomatino"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"/>
</LinearLayout>